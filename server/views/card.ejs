<div class="col s12 m4">
  <div class="card">
    <div class="card-image">
      <img src="<%= image %>">
      <span class="card-title"><%= title %></span>
    </div>
    <div class="card-content">
      <p><%= description %></p>
    </div>
    <div class="card-action">
      <a href="/product/<%- id %>"><%= price %></a>
      <button class="btn teal waves-effect waves-light right">Login</button>
    </div>
  </div>
</div>
