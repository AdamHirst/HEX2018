<div class="col s12 m4">
  <div class="card">
    <div class="card-image">
      <img src="<%= image %>">
      <span class="card-title"><%= title %></span>
    </div>
    <div class="card-content">
      <p><%= description %></p>
    </div>
    <div class="card-action">
      <a href="#"><%= action %></a>
    </div>
  </div>
</div>
